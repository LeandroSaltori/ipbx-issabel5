{
   "background": {
      "persistent": false,
      "scripts": [ "js/jquery-1.8.0.min.js", "js/bg.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon128.png",
      "default_popup": "popup.html",
      "default_title": "Discador IPbx Prisma Telecom"
   },
   "content_scripts": [ {
      "js": [ "js/fg.js" ],
      "matches": [ "*://*/*" ]
      
   } ],
   "description": "Faça uma chamada para seu ramal a partir do texto selecionado. Com esta extensão, você pode copiar o número e enviar a discagem para o seu ramal pelo seu navegador, ou mesmo selecionar o número e clicar com o botão direito sobre ele para enviar a discagem.",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "manifest_version": 2,
   "name": "Discador IPbx - Prisma Telecom",
   "options_page": "options.html",
   "permissions": [ "contextMenus", "tabs", "*://*/*" ],
   "short_name": "Discador Prisma",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
